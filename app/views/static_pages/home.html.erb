<% if logged_in? %>
  <h1>Hello, <%= current_user.name %> </h1>
  <% attendances.attended_events.upcoming.each do |e| %>
  <ul>
    <li>
      <p><%= e.event %></p>
    </li>
  </ul>
<% end %>

<h1> Past Events: <h1>

<% attendaces.attended_events.past.each do |e| %>
  <ul>
    <li>
      <p><%= e.event %></p>
    </li>
  </ul>
<% end %>
<% else %>
  <p>Sign Up, first</p>
<% end %>