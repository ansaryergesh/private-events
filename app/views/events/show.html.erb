<div class="row">
 <div class="col-md-6 col-md-offset-3">

<h1><%= @event.description.upcase %> </h1>
<div>
	<p><b>Event creator: </b><%= @event.creator.name %></p>
	<p><b>Date: </b><%= @event.date.strftime("%B %e, %Y") %></p>
  <p><b>Where: </b><%= @event.location %></p>
</div>

<hr>

<h1> Attedancees: <h1>
  <% @event.attendees.each do |e| %>
		<ul>
		<li>
			<%= e.name %>
		</li>
		</ul>
  <%end%>





 </div>
 </div>